package com.unrc.app;

import com.unrc.app.models.Owner;
import com.unrc.app.models.Address;
import org.javalite.activejdbc.Base;
import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

public class Inmo {
    public static void main( String[] args )
    {
        Base.open("com.mysql.jdbc.Driver", "jdbc:mysql://localhost/inmoapp_development", "root", "Pantera1.");

       	Owner e = new Owner();
        e.set("email", "user@email.com");
        e.set("first_name", "John");
        e.set("last_name", "Doe");
		e.set("dni","123456");
        e.saveIt();

		Address a = new Address();
		e.add(a);
		a.set("street","gral bustos");
		a.set("num","1234");
		a.set("neighborhood","azucarSintactica");
		a.saveIt();
	
        System.out.println( "Hello World!" );
    }	
}
